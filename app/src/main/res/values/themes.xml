<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FISH" parent="Theme.AppCompat.Light">

        <!-- Primary brand color. -->
        <item name="colorPrimary">#C06AFD</item>
        <item name="colorPrimaryVariant">#C06AFD</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#C06AFD</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.FISH.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.FISH.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FISH.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>